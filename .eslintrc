{
    "extends": [
        "airbnb/base"
    ],
    "rules": {
        "linebreak-style": 0,
        "no-alert": "off",
        "no-console": "off",
        "indent": [
            1,
            "tab"
        ],
        "no-tabs": 0,
        "no-mixed-spaces-and-tabs": [
            2,
            "smart-tabs"
        ],
        "no-undef": 0,
        "no-plusplus": 0,
        "import/prefer-default-export": "off",
        "comma-dangle": 0,
        "no-trailing-spaces": 0,
        "arrow-parens": 0,
        "arrow-body-style": 0,
        "eol-last": 0
    }
}